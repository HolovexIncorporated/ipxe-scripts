#!ipxe
isset ${server-ip} || set server-ip 192.168.1.9

kernel https://download.opensuse.org/distribution/leap/15.0/repo/oss/boot/x86_64/loader/linux splash=silent install=https://download.opensuse.org/distribution/leap/15.0/repo/oss/ initrd=initrd
initrd https://download.opensuse.org/distribution/leap/15.0/repo/oss/boot/x86_64/loader/initrd
boot || imgfree
echo -n Boot failed
chain http://${server-ip}/boot/linux/opensuse.ipxe
